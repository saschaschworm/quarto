{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint:check": {
      "dependsOn": ["^lint:check"]
    },
    "lint:fix": {
      "dependsOn": ["^lint:fix"]
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env", ".env*.local"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**", "_extensions/voting/**"]
    },
    "start": {
      "dependsOn": ["^start"]
    }
  }
}
