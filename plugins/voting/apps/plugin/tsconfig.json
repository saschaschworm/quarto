{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "strict": true,
    "target": "es2017",
    "lib": ["es2017", "dom", "dom.iterable"],
    "esModuleInterop": true,
    "allowJs": true,
    "skipLibCheck": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "removeComments": true,
    "outDir": "_extensions/voting"
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
